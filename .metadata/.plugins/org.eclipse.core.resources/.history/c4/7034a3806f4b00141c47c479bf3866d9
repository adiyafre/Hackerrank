package xor_subsequence;

import java.util.Scanner;
import java.math.BigInteger;  




public class Solution {


	public static void main(String[] args) {
		
		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
	
		
		int[] a = new int[n];
		
			
		
		for (int j = 0; j<n; j++){
		
			
			int k = sc.nextInt();
			a[j] = k;
			
		
		}
		
		
		
		 int prev = a[0];
		 int   cont_sum = prev;
		    for(int i=1;i<9;i++)
		    {
		        if(a[i] != prev)
		        {
		           System.out.printf("contiguous sub sequence sum: %d\n", cont_sum);
		            prev = a[i];
		            cont_sum = prev;
		        }
		        else
		        {
		            prev = a[i];
		            cont_sum += prev;
		        }
		    }
		    System.out.printf("contiguous sub sequence sum: %d\n", cont_sum);
		
		
				
		
		for (int i=0; i<n; i++){
			
			System.out.println(a[i]);
		}
		
		
	}

}
