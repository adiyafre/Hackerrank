package grid_Search;

import java.util.Scanner;


public class Solution {


	
	public static void main(String[] args) {
		
		

		
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();

		int R = sc.nextInt();

		int C = sc.nextInt();

		
		
		String[] pattern = new String[R];
				
			
		sc.nextLine(); 
		
		for (int i = 0; i<R; i++){
			pattern[i] = sc.nextLine(); 
			
		}
		
		
		int r = sc.nextInt();

		int c = sc.nextInt();

		sc.nextLine();
		
		String[] look = new String[r];
				
				
		for (int i = 0; i<R; i++){
			look[i] = sc.nextLine(); 
			System.out.println(look[i].toString());
			
		}
		
		
		
		
		
		for (int i = 0; i<R; i++){
			if (pattern[i].contains(look[0])) System.out.println(look[0].toString());
			
		}
		
		
			


		
	}

}
