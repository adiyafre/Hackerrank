package Lover;

import java.util.Scanner;




public class Solution {


	
	public static void main(String[] args) {
		
		
		
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();

		
		

			int n = sc.nextInt();
			int k = sc.nextInt();
			int[][] a = new int[n+1][n+1];

		
			
			for (int i=1; i<=n; i++)
				a[i][1] = i;
			
			
		

	
	
	
	for (int i=1; i<=n; i++){
		for (int j=1; j<=n; j++)
			System.out.print(a[i][j]+" ");
	
	 System.out.println();
	}
	
		
	}
}


