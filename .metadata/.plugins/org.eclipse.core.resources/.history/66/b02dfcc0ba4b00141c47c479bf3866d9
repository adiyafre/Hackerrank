package grid_Search;

import java.util.Scanner;


public class Solution {


	
	public static void main(String[] args) {
		
		

		
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();

		int R = sc.nextInt();

		int C = sc.nextInt();

		
		String[] pattern = new String[R];
				
				
		for (int i = 0; i<R; i++){
			pattern[i] = sc.nextLine(); 
			System.out.println(i+":"+pattern[i].toString());
			
		}
		
		
		int r = sc.nextInt();

		int c = sc.nextInt();

		
		String[] look = new String[r];
				
				
		for (int i = 0; i<R; i++){
			look[i] = sc.nextLine(); 
			
			
		}
		
		
		
		for (int i = 0; i<R; i++){
			 for (int j = 0; j<C; j++)
				
				System.out.println(look[i].toString());

			 System.out.println();
			}
			
			
			


		
	}

}
