package Lover;

import java.util.Scanner;




public class Solution {


	
	public static void main(String[] args) {
		
		
		
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();

		
		

			int n = sc.nextInt();
			int k = sc.nextInt();
			int[][] a = new int[n+1][k+1];


			  a[1][1] = 1;
			  a[2][1] = 2;
			  for(int i = 3; i <= n; i++){
			   for(int j = 1; j <= ((i+1)/2); j++ ){
			    if (j==1) a[i][j]=i;
			    else a[i][j]=(a[i-1][j]+a[i-2][j-1]) % 100003; 
			   }
			  }
			  System.out.println(a[n][k]);
			  /*for(int i = 1; i <= n; i++){
			   for(int j = 1; j <= n; j++ ){
			    System.out.print(a[i][j]+" ");
			   }
			   System.out.println();
			  }*/
			 
			
			
			
	
	
	for (int i=1; i<=n; i++){
		for (int j=1; j<=n; j++)
			System.out.print(a[i][j]+" ");
	
	 System.out.println();
	}
	
		
	}
}


