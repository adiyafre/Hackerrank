package almost_Sorted;

import java.util.Scanner;


public class Solution {


	
	public static void main(String[] args) {
		
		

		
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();

		int[] a = new int[n];

		
		for (int t = 0; t<n; t++){
		
			
			a[t] = sc.nextInt();


			
		
		}
		



	


		
	}

}
